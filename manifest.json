{
  "manifest_version": 2,
  "name": "dailyF1",
  "description": "Get latest updates for your daily dose of F1 on your Google homepage",
  "version": "1",
  "author": "Prateek Celly",
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "New Tab"
  },
  "icons": [
    {
      "logo": "logo.png"
    }
  ],
  "chrome_url_overrides": { "newtab": "index.html" },
  "permissions": ["activeTab", "geolocation"]
}
